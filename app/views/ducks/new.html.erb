<% if flash[:errors] %>
  <% flash[:errors].each do |ms| %>
    <p><%= ms %></p>
  <% end %>
<% end %>
<%= form_for @duck do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.collection_select(:student_id, @students, :id, :name) %>
  <%= f.submit %>
<% end %>